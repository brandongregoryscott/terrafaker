{
    "author": "Brandon Scott",
    "bin": {
        "terrafaker": "bin/run.js"
    },
    "bugs": "https://github.com/brandongregoryscott/terrafaker/issues",
    "dependencies": {
        "@faker-js/faker": "10.1.0",
        "@inquirer/prompts": "8.0.2",
        "@oclif/core": "4.8.0",
        "@oclif/plugin-help": "6.2.36",
        "lodash-es": "4.17.21",
        "terraform-generator": "6.4.4",
        "zx": "8.8.5"
    },
    "description": "CLI for easily generating fake terraform files and repos",
    "devDependencies": {
        "@eslint/compat": "1.4.1",
        "@oclif/test": "4.1.15",
        "@stylistic/eslint-plugin": "5.6.1",
        "@types/fs-extra": "11.0.4",
        "@types/lodash-es": "4.17.12",
        "@types/node": "24.10.3",
        "eslint": "9.39.1",
        "eslint-plugin-collation": "1.5.0",
        "oclif": "4.22.54",
        "prettier": "3.6.1",
        "shx": "0.3.4",
        "ts-node": "10.9.2",
        "typescript": "5.9.3",
        "typescript-eslint": "8.49.0",
        "vitest": "4.0.15"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "files": [
        "./bin",
        "./dist",
        "./oclif.manifest.json"
    ],
    "homepage": "https://github.com/brandongregoryscott/terrafaker",
    "keywords": [
        "oclif"
    ],
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "name": "terrafaker",
    "oclif": {
        "bin": "terrafaker",
        "commands": "./dist/commands",
        "dirname": "terrafaker",
        "plugins": [
            "@oclif/plugin-help"
        ],
        "topicSeparator": " "
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/brandongregoryscott/terrafaker.git"
    },
    "scripts": {
        "build": "npm run clean && tsc -b",
        "build:watch": "npm run clean && tsc --watch",
        "clean": "shx rm -rf dist tsconfig.tsbuildinfo",
        "format": "prettier . --check",
        "format:fix": "prettier . --write",
        "generate:manifest": "oclif manifest",
        "generate:readme": "oclif readme && prettier README.md --write",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "postbuild": "npm run generate:manifest",
        "postpack": "shx rm -f oclif.manifest.json",
        "posttest": "npm run lint",
        "prepack": "npm run generate:manifest && npm run generate:readme",
        "test": "vitest",
        "version": "npm run generate:readme && git add README.md"
    },
    "type": "module",
    "types": "dist/index.d.ts",
    "version": "0.0.1"
}
